{
  "expo": {
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*"
    ],
    "name": "Kiddie Kredit",
    "slug": "kiddiekredit",
    "scheme": "kiddiekredit",
    "icon": "assets/images/kk-square1024.png",
    "privacy": "unlisted",
    "description": "Kidde Kredit App for teaching financial literacy to children via chore tracking",
    "orientation": "portrait",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "enabled": true
    },
    "ios": {
      "bundleIdentifier": "com.kiddiekredit.app",
      "buildNumber": "23",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "permissions": ["NSLocationWhenInUseUsageDescritption", "NSLocationAlwaysAndWhenInUseUsageDescription"],
        "NSAppTransportSecurity": {
          "NSAllowsLocalNetworking": true,
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "package": "com.kiddiekredit.app",
      "versionCode": 22,
      "permissions": ["ACCESS_COURSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "version": "1.2.0"
  }
}
